<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" /> <!-- Adjust the radius value as desired -->
            <solid android:color="#dbe7f0" /> <!-- Change color value as desired -->
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="10dp" /> <!-- Adjust the radius value as desired -->
                <solid android:color="@color/green" /> <!-- Change color value as desired -->
            </shape>
        </clip>
    </item>
</layer-list>